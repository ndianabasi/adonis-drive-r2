{
  "name": "adonis-drive-r2",
  "version": "1.0.0",
  "description": "R2 driver for AdonisJS drive",
  "main": "build/providers/R2Provider.js",
  "files": [
    "build/adonis-typings",
    "build/instructions.md",
    "build/src",
    "build/providers"
  ],
  "types": "build/adonis-typings/index.d.ts",
  "scripts": {
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "lint": "eslint . --ext=.ts --cache",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && copyfiles instructions.md build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json ndianabasi/adonis-drive-r2",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "format": "prettier --write --cache .",
    "commit": "git-cz"
  },
  "author": "Ndianabasi Udonkang",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@aws-sdk/lib-storage": "^3.186.0",
    "@aws-sdk/s3-request-presigner": "^3.186.0",
    "get-stream": "^6.0.1"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@japa/assert": "^1.3.6",
    "@japa/run-failed-tests": "^1.1.0",
    "@japa/runner": "^2.2.2",
    "@japa/spec-reporter": "^1.3.2",
    "@types/node": "^18.11.11",
    "commitizen": "^4.2.6",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.2",
    "mrm": "^4.1.13",
    "np": "^7.6.2",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
   "peerDependencies": {
    "@adonisjs/core": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "env": {
      "R2_KEY": "dummyKey",
      "R2_SECRET": "dummySecret",
      "R2_ACCOUNT_ID": "dummySecret",
      "R2_BUCKET": "dummyBucket",
      "R2_PUBLIC_URL": "dummyEndpoint"
    },
    "types": "adonis-drive-r2",
    "providers": [
      "adonis-drive-r2"
    ]
  },
   "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndianabasi/adonis-drive-r2.git"
  },
  "keywords": [
    "adonisjs",
    "adonis-drive",
    "r2"
  ],
  "bugs": {
    "url": "https://github.com/ndianabasi/adonis-drive-r2/issues"
  },
  "homepage": "https://github.com/ndianabasi/adonis-drive-r2#readme",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
